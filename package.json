{
  "name": "ts-fusion-parser",
  "version": "0.1.3",
  "description": "Parser for Neos Fusion Files",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^17.0.31",
    "assemblyscript": "^0.25.2",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "test": "node tests",
    "prepublishOnly": "rm -rf out && tsc",
    "watch-ts": "tsc --watch",
    "watch-src": "nodemon out/main.js",
    "watch-afx": "nodemon out/afx.js",
    "watch-eel": "nodemon out/eel.js",
    "version-patch": "npm version patch && git push --tags && git push --all",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild": "yarn run asbuild:debug && yarn run asbuild:release",
    "start": "npx serve ."
  },
  "main": "out/lib.js",
  "author": "Simon Schmidt",
  "license": "AGPL-3.0-or-later",
  "repository": "git@gitlab.com:sjsone/ts-fusion-parser.git",
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  }
}